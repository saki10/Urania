      <%= form_with model: @uranai do |form| %>

      <%= form.collection_select(:birthday_month, BirthMonth.all, :id, :birth_months) %>
      <%= form.collection_select(:blood_type, BloodType.all, :id, :blood_types)  %>

      <%= form.text_area :money, rows: "20" %>

      <%= form.text_area :health,  rows: "20" %>

      <%= form.text_area :love, rows: "20" %>

      <%= form.submit "送信" %>
<% end %>